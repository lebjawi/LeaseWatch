{
  "name": "leasewatch",
  "version": "1.0.0",
  "description": "Automated Apartment Pricing Tracker for Camden Dunwoody & Perimeter Gardens",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/index.js",
    "dev": "ts-node index.ts",
    "dev:watch": "ts-node --watch index.ts",
    "server": "ts-node src/server.ts",
    "server:build": "npm run build && node dist/src/server.js",
    "watch": "tsc --watch",
    "clean": "rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "apartment",
    "pricing",
    "tracker",
    "puppeteer",
    "scraper",
    "automation",
    "real-estate",
    "typescript"
  ],
  "author": "lebjawi",
  "license": "MIT",
  "dependencies": {
    "playwright": "^1.54.2",
    "puppeteer": "^21.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.5.0",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lebjawi/LeaseWatch.git"
  },
  "bugs": {
    "url": "https://github.com/lebjawi/LeaseWatch/issues"
  },
  "homepage": "https://github.com/lebjawi/LeaseWatch#readme",
  "engines": {
    "node": ">=16.0.0"
  }
}
